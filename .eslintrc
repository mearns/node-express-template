{
    "extends": [
        "standard",
        "plugin:prettier/recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "semi": [2, "always"],
        "no-extra-semi": 2,
        "semi-spacing": [2, { "before": false, "after": true }],
        "prettier/prettier": "error"
    }
}
